{
  "name": "react-typescript-meteor",
  "private": true,
  "scripts": {
    "commit": "git-cz",
    "start": "meteor run",
    "test": "npm run test:jest",
    "test:jest": "jest",
    "test:jest:watch": "jest --watchAll",
    "type:imports": "tslint -p ./tsconfig.json --type-check './imports/**/*.{ts,tsx}'",
    "type:client": "tslint -p ./tsconfig.json --type-check './client/**/*.{ts,tsx}'",
    "type:server": "tslint -p ./tsconfig.json --type-check './server/**/*.ts'",
    "type": "npm run type:client && npm run type:server && npm run type:imports",
    "precommit": "npm run test"
  },
  "dependencies": {
    "babel-runtime": "6.20.0",
    "meteor-node-stubs": "0.2.4",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-komposer": "2.0.0",
    "react-router-dom": "4.1.2",
    "simpl-schema": "0.3.2"
  },
  "devDependencies": {
    "@types/enzyme": "2.8.4",
    "@types/jest": "20.0.6",
    "@types/meteor": "1.4.2",
    "@types/react": "15.6.0",
    "@types/react-dom": "15.5.1",
    "@types/react-router-dom": "4.0.7",
    "babel-jest": "20.0.3",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "cz-conventional-changelog": "2.0.0",
    "enzyme": "2.9.1",
    "jest": "20.0.4",
    "jest-cli": "^20.0.4",
    "jest-meteor-stubs": "1.5.1",
    "meteor-husky": "^0.14.6",
    "react-test-renderer": "15.6.1",
    "ts-jest": "20.0.9",
    "tslint": "5.5.0",
    "tslint-react": "3.1.0",
    "typescript": "2.4.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
